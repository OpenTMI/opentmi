    "junit-xml-parser": "0.0.3",
    "jwt-simple": "^0.5.6",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "mime": "^2.6.0",
    "moment": "^2.29.3",
    "mongodb-memory-server": "^8.4.2",
    "mongoose": "^6.2.9",
    "mongoose-query": "^0.7.4",
    "mongoose-schema-jsonschema": "^2.0.2",
    "nconf": "^0.12.0",
