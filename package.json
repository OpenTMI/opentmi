{
  "name": "OpenTMI",
  "description": "OpenTMI - Open Source Test Management Infrastructure",
  "version": "0.12.1",
  "author": {
    "name": "Jussi Vatjus-Anttila",
    "email": "jussiva@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenTMI/opentmi.git"
  },
  "keywords": [
    "IoT",
    "hardware",
    "opentmi",
    "test",
    "management",
    "backend"
  ],
  "copyright": "Jussi Vatjus-Anttila <jussiva@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">10.0.0"
  },
  "scripts": {
    "start": "node app/index.js",
    "healthcheck": "node app/tools/healthcheck.js",
    "cluster": "node app/cluster.js",
    "debug": "node --inspect index.js",
    "debug-cluster": "node --inspect app/cluster.js",
    "test": "NODE_ENV=test npm run test-unit-spec",
    "test-unit-ci": "npm run test-unit-lcov && npm run test-unit-json",
    "test-unit-json": "nyc mocha --recursive --reporter json --reporter-option output=report_unit.json test/unittests",
    "test-unit-lcov": "nyc --reporter lcov mocha --recursive test/unittests",
    "test-api": "grunt",
    "lint": "node node_modules/eslint/bin/eslint .",
    "lint-fix": "node node_modules/eslint/bin/eslint . --fix",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "dependencies": {
    "async": "^3.2.3",
    "bcryptjs": "^2.4.3",
